<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
        <display-name>jQuery_Ajax_Request</display-name>
        <welcome-file-list>
            <welcome-file>CardDetails.html</welcome-file>
            <welcome-file>index.htm</welcome-file>
            <welcome-file>index.jsp</welcome-file>
            <welcome-file>default.html</welcome-file>
            <welcome-file>default.htm</welcome-file>
            <welcome-file>default.jsp</welcome-file>
        </welcome-file-list>
    <servlet>
        <description>Get Information about Country - jQuery Ajax Request</description>
        <display-name>ItemsInformationByCardTab</display-name>
        <servlet-name>ItemsInformationByCardTab</servlet-name>
        <servlet-class>com.as400samplecode.ItemsInformationByCardTab</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ItemsInformationByCardTab</servlet-name>
        <url-pattern>/ItemsInformationByCardTab</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>Get Information about Country - jQuery Ajax Request</description>
        <display-name>OnLoad</display-name>
        <servlet-name>OnLoad</servlet-name>
        <servlet-class>com.as400samplecode.OnLoad</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OnLoad</servlet-name>
        <url-pattern>/OnLoad</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>Get Information about Country - jQuery Ajax Request</description>
        <display-name>ItemsInformation</display-name>
        <servlet-name>ItemsInformation</servlet-name>
        <servlet-class>com.as400samplecode.ItemsInformation</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ItemsInformation</servlet-name>
        <url-pattern>/ItemsInformation</url-pattern>
    </servlet-mapping>

</web-app>